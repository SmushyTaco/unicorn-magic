{
	"name": "@smushytaco/unicorn-magic",
	"version": "1.0.1",
	"description": "Some useful utilities I often need",
	"license": "MIT",
	"private": false,
	"author": {
		"name": "SmushyTaco",
		"email": "personal@nikanradan.com"
	},
	"type": "module",
	"exports": {
		"node": {
			"import": {
				"types": "./dist/node.d.mts",
				"default": "./dist/node.mjs"
			},
			"require": {
				"types": "./dist/node.d.cts",
				"default": "./dist/node.cjs"
			}
		},
		"default": {
			"import": {
				"types": "./dist/default.d.mts",
				"default": "./dist/default.mjs"
			},
			"require": {
				"types": "./dist/default.d.cts",
				"default": "./dist/default.cjs"
			}
		}
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/SmushyTaco/unicorn-magic.git"
	},
	"bugs": {
		"url": "https://github.com/SmushyTaco/unicorn-magic/issues"
	},
	"engines": {
		"node": ">=18.12.0",
		"npm": ">=8.19.2"
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"clean": "npx del-cli dist",
		"build": "npm run clean && npx vite build",
		"prepare": "npm run build"
	},
	"files": [
		"dist",
		"LICENSE",
		"README.md"
	],
	"keywords": [
		"utilities",
		"util",
		"extras",
		"url",
		"path",
		"delay",
		"wait",
		"settimeout",
		"sleep",
		"child_process",
		"child",
		"process",
		"subprocess",
		"exec",
		"execfile",
		"execfilesync"
	],
	"dependencies": {
		"promisify-child-process": "^4.1.2"
	},
	"devDependencies": {
		"@types/node": "^22.10.5",
		"del-cli": "^6.0.0",
		"@eslint/js": "^9.17.0",
		"@smushytaco/versionchecker": "^1.0.4",
		"@typescript-eslint/eslint-plugin": "^8.19.0",
		"@typescript-eslint/parser": "^8.19.0",
		"typescript-eslint": "^8.19.0",
		"vite": "^6.0.7",
		"vite-plugin-tsc-transpile": "^1.0.3",
		"vite-plugin-dts": "^4.4.0",
		"globals": "^15.14.0",
		"prettier": "^3.4.2",
		"typescript": "^5.7.2"
	}
}
